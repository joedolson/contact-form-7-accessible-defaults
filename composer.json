{
    "name": "joedolson/contact-form-7-accessible-defaults",
    "description": "Contact Form 7: Accessible Defaults",
    "keywords": ["wcag", "wpcf7", "accessibility", "form"],
    "homepage": "https://github.com/joedolson/contact-form-7-accessible-defaults",
    "license": "GPL-2.0+",
    "authors": [{
        "name": "Joseph Dolson",
        "email": "plugins@joedolson.com",
        "homepage": "https://www.joedolson.com"
    }],
    "type": "wordpress-plugin",
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "^3.5",
        "wp-coding-standards/wpcs": "^2.2",
        "wp-phpunit/wp-phpunit": "^5.8",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "require": {
        "composer/installers": "~1.0"
    },
    "scripts": {
        "format": "phpcbf --standard=phpcs.xml.dist --report-summary --report-source",
        "lint": "phpcs --standard=phpcs.xml.dist",
        "test": "phpunit -c phpunit.xml.dist --verbose"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
